            >
                <!-- Bloc pour chaque grand prix -->

                <% tracks.forEach(element => { %>
                <div class="bg-white rounded-lg shadow-md">
                    <img
                        src="img/vroumvroum/<%= element.idTrack %>.png"
                        alt="<%= element.name %>"
                        class="w-full h-48 object-cover rounded-t-lg"
                    />
                    <div class="p-4">
                        <h2 class="text-xl font-semibold mb-2">
                            <%= element.name %>
                        </h2>
                        <p class="text-gray-700 mb-2">
                            Pays: <%= element.country %>
                        </p>
                        <p class="text-gray-700 mb-2">
                            Distance: <%= element.oneLapDistance %>km
                        </p>
                        <p class="text-gray-700 mb-2">
                            Distance totale: <%= element.totalDistance %>km
                        </p>
                        <p class="text-gray-700 mb-2">
                            Nombre de tours: <%= element.lapsNumber %>
                        </p>
                        <p class="text-gray-700 mb-2">
                            Record du tour: <%= element.lapRecord %>
                        </p>
                        <p class="text-gray-700 mb-2">
                            Année de création: <%= element.creationYear %>
                        </p>
                        <button
                            class="text-white bg-blue-500 py-1 px-2 rounded hover:bg-blue-600"
                            onclick="showDetails('<%= element.idTrack %>')"
                        >
                            Détails
                        </button>
                    </div>
                </div>
                <% }); %>
