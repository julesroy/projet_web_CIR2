<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <link rel="shortcut icon" href="/img/logo/f1.svg" type="image/x-icon" />
        <script src="https://cdn.tailwindcss.com"></script>
        <title>Classement</title>
    </head>
    <body>
        <%- include('./includes/header.ejs') %>
        <br />
        <div class="flex justify-center text-2xl">Classement total</div>
        <main class="flex flex-col items-center">
            <% tableauClassement.forEach(element => { %>
            <div
                class="w-1/2 mt-8 flex items-center gap-4 rounded-lg border border-gray-100 bg-white p-6 sm:justify-between"
            >
                <span>id: <%= element[0] %></span
                ><span><%= element[1] %> points</span>
            </div>
            <% }); %>
        </main>
    </body>
</html>
